{
  "name": "@trigo/atrix-acl",
  "version": "4.0.1",
  "engines": {
    "node": ">=9.6.0"
  },
  "config": {
    "loadOnStart": true
  },
  "description": "ACL for atrix http endpoints ",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 30000 $(find . -name '*.specs.js' -not -path './node_modules/*')",
    "lint": "eslint lib",
    "yarn": "yarn install",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "prettify": "prettier --write '**/*.{js,jsx}'"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier",
      "git add"
    ]
  },
  "keywords": [
    "atrix"
  ],
  "author": "trigo Gmbh <office@trigo.at>",
  "license": "ISC",
  "dependencies": {
    "@trigo/atrix-swagger": "^2.1.0",
    "bluebird": "^3.5.0",
    "boom": "^7.3.0",
    "joi": "^14.3.0",
    "joi-date-extensions": "^1.0.2",
    "jsonwebtoken": "^8.4.0",
    "node-fetch": "^2.1.1",
    "ramda": "^0.24.1",
    "route-parser": "0.0.5",
    "shot": "^4.0.7"
  },
  "devDependencies": {
    "@trigo/atrix": "^6.0.0-12",
    "chai": "^4.0.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-trigo": "^6.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.15.3",
    "supertest": "^3.0.0",
    "uuid": "^3.1.0"
  }
}
